{
  "name": "@solid/authenticator-node",
  "version": "0.0.0",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "test": "npm run lint && npm run node-unit-test && npm run node-acceptance-test",
    "lint": "eslint src/**/*.ts && eslint __tests__/**/*.ts",
    "lint-fix": "prettier --check \"src/**/*.ts\"",
    "unit-test": "npm run node-unit-test",
    "acceptance-test": "npm run node-acceptance-test",
    "node-test": "npm run test",
    "node-unit-test": "jest --coverage --verbose -c jest.unit.config.js",
    "node-acceptance-test": "jest --verbose -c jest.acceptance.config.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@solid/authenticator-core": "^0.0.0",
    "jose": "^1.19.0",
    "tsyringe": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
